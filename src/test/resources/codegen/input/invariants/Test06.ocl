package invariants;

ocl Test06 {
  inv(Time time):
    time != null;

  context Company c inv:
    c.name != "";

  context Company inv:
    this.name.length() > 0 &&
    name.length() > 0;

  context Auction a inv:
    a.getMessages().size() > 0 &&
    a.getBidders().size() > 0;

  context Company c inv:
    !c.name.replace("Company", "").contains("Company");
}
